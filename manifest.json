{
  "name": "Chronicle",
  "manifest_version": 2,
  "version": "0.0.1",
  "content_security_policy": "script-src; object-src 'self'; img-src chrome://favicon",
  "description": "A pop-up toolbar menu of the recent history within the current site",
  "options_page": "options.html",
  "icons": {
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "activeTab", "history", "chrome://favicon/"]
}
